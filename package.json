{
    "name": "eventbrite-api",
    "version": "1.0.1",
    "main": "index.js",
    "scripts": {
        "deploy": "now alias $(now --name $INSTANCE_NAME -e CACHE_EXPIRATION=$CACHE_EXPIRATION -e NODE_ENV=production -e ORGANIZERS=$ORGANIZERS -e TOKEN=$TOKEN --npm --token $NOW_TOKEN) $INSTANCE_NAME --token=$NOW_TOKEN",
        "dev": "micro-dev -p ${PORT:-4003}",
        "precommit": "eslint '**/*.js'",
        "scale": "now scale $INSTANCE_NAME'.now.sh' sfo --token $NOW_TOKEN",
        "start": "micro"
    },
    "author": "durancristhian@gmail.com",
    "license": "MIT",
    "dependencies": {
        "dotenv": "^6.0.0",
        "got": "^8.3.1",
        "memory-cache": "^0.2.0",
        "micro": "^9.3.2",
        "micro-cors": "^0.1.0",
        "nodemon": "^1.17.5"
    },
    "devDependencies": {
        "babel-eslint": "^8.2.3",
        "eslint": "^4.19.1",
        "husky": "^0.14.3",
        "micro-dev": "^3.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/meetupjs-ar/eventbrite-api.git"
    }
}
